/* ---------------------------------弹框 */
.tips {display: none;position: fixed;background: #000;opacity: 0.5; top: 0;left: 0;right: 0;bottom: 0;z-index: 999;}
.dele-block {visibility: collapse; opacity: 0;  position: fixed;bottom: 28rpx; background: #fff; width: 698rpx; left: 26rpx; height: 338rpx;border-radius: 10rpx;z-index: 999;  text-align: center; color: #424242;font-size: 34rpx; transition: all .3s ease-in-out;}
.dele-block .title {height: 257rpx; line-height: 257rpx;font-weight: bold;}
.dele-block .fot {display: flex; border-top: 1px solid #EDEDED;}
.dele-block .fot view {flex: 1;line-height: 80rpx; height: 80rpx;}
.dele-block .fot view:nth-of-type(1) {border-right: 1px solid #EDEDED;}
.dele-block-wrap .dele-block {  visibility: visible; opacity: 1;}
.dele-block-wrap .tips { display: block;}