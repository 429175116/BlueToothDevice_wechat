.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  background: #EDEDED;
   padding: 0;
  margin: 0;
}
#getUserInfo{
  position: fixed; 
  top: 0; 
  left: 0; 
  bottom: 0; 
  right: 0; 
  opacity: 0; 
  z-index: 999; 
}
.overflow-two {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}

.overflow-one {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}





